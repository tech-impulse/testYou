<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.www" versionCode="10" version="1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>Youtter</name>

    <description>
        An example for phonegap build docs.
    </description>

    <author href="https://youtter.com" email="javier.fernandez@tech-impulse.com">
        Hardeep Shoker
    </author>

    <preference name="Fullscreen" value="true" />

    <gap:config-file platform="ios" parent="UISupportedInterfaceOrientations" overwrite="true">
        <array>
            <string>UIInterfaceOrientationLandscapeOmg</string>
        </array>
    </gap:config-file>

    <gap:config-file platform="android" parent="/manifest">
        <supports-screens android:xlargeScreens="false" android:largeScreens="false" android:smallScreens="false" />
    </gap:config-file>

    <gap:plugin name="org.apache.cordova.camera" version="0.2.9" />

</widget>


